<div class="row" id="tablet-view">
  <div class="span3" id="posts-section"> 
		<ul class="posts">
		  <% postsIndex.each do |post| %>
		  <li class="post">
				<a href="<%= post[2] %>"><img src="<%= post[3]%>" alt="<%= post[1]%>" title="<%= post[1] %>"></a>
				<p><a href="<%= post[2] %>"><%= post[1]%></a></p>
		  </li>
	    <% end %>
		</ul>
  </div>
	<div class="span9" id="post-container"> 
